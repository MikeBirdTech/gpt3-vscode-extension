{
  "name": "gpt3-vscode-extension",
  "displayName": "GPT-3 A.I. Coding Assistant",
  "description": "Use GPT-3 in VS Code! Generate code, documentation, code reviews, and more!",
  "version": "0.11.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Machine Learning",
    "Education",
    "Testing",
    "Debuggers"
  ],
  "keywords": [
    "AI",
    "OpenAI",
    "GPT-3",
    "GPT3",
    "GPT"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:GPT.createDocs",
    "onCommand:GPT.suggestImprovement",
    "onCommand:GPT.askGPT"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "GPT.createDocs",
        "title": "Create Documentation",
        "icon": "$(hubot)"
      },
      {
        "command": "GPT.createCodeFromDocs",
        "title": "Create code from docs",
        "icon": "$(check)"
      },
      {
        "command": "GPT.suggestImprovement",
        "title": "GPT Code Review",
        "icon": "$(rocket)"
      },
      {
        "command": "GPT.askGPT",
        "title": "Ask GPT",
        "icon": "$(star)"
      },
      {
        "command": "GPT.updateAPIKey",
        "title": "Update OpenAI API Key",
        "icon": "$(pencil)"
      },
      {
        "command": "GPT.removeAPIKey",
        "title": "Remove OpenAI API Key from secret storage",
        "icon": "$(error)"
      }
    ],
    "configuration": {
      "title": "GPT",
      "properties": {
        "GPT.org": {
          "type": "string",
          "default": null,
          "markdownDescription": "The [organization](https://beta.openai.com/account/org-settings) value from OpenAI"
        },
        "GPT.model": {
          "type": "string",
          "default": "text-davinci-003",
          "markdownDescription": "The OpenAI [model](https://beta.openai.com/docs/models) to use"
        },
        "GPT.maxTokens": {
          "type": "number",
          "default": 250,
          "minimum": 1,
          "markdownDescription": "The maximum number of [tokens](https://beta.openai.com/docs/introduction/overview) for each request to GPT"
        },
        "GPT.temperature": {
          "type": "number",
          "default": 0.3,
          "minimum": 0,
          "maximum": 1,
          "markdownDescription": "The [temperature](https://beta.openai.com/docs/api-reference/completions/create#completions/create-temperature) of each request to GPT. Must be between 0 and 1"
        },
        "GPT.codeModel": {
          "type": "string",
          "default": "code-davinci-002",
          "markdownDescription": "The OpenAI [model](https://beta.openai.com/docs/models) to use"
        },
        "GPT.codeMaxTokens": {
          "type": "number",
          "default": 250,
          "minimum": 1,
          "markdownDescription": "The maximum number of [tokens](https://beta.openai.com/docs/introduction/overview) for each request to GPT"
        },
        "GPT.codeTemperature": {
          "type": "number",
          "default": 0.1,
          "minimum": 0,
          "maximum": 1,
          "markdownDescription": "The [temperature](https://beta.openai.com/docs/api-reference/completions/create#completions/create-temperature) of each request to GPT. Must be between 0 and 1"
        }
      }
    },
    "keybindings": [
      {
        "command": "GPT.createDocs",
        "key": "alt+d",
        "mac": "option+d",
        "when": "editorTextFocus"
      },
      {
        "command": "GPT.suggestImprovement",
        "key": "alt+i",
        "mac": "option+i",
        "when": "editorTextFocus"
      },
      {
        "command": "GPT.askGPT",
        "key": "alt+g",
        "mac": "option+g",
        "when": "editorTextFocus"
      },
      {
        "command": "GPT.createCodeFromDocs",
        "key": "alt+x",
        "mac": "option+x",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arrendy/gpt3-vscode-extension.git"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "author": "Mike Bird",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Arrendy/gpt3-vscode-extension/issues"
  },
  "homepage": "https://github.com/Arrendy/gpt3-vscode-extension#readme",
  "dependencies": {
    "openai": "^3.1.0"
  },
  "pricing": "Free",
  "publisher": "Arrendy",
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#D79D58"
  }
}
